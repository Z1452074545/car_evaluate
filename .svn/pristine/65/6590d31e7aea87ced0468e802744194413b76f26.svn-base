
.title{
    width: 100%;
    height: 1rem;
    text-align: center;
    line-height: 1rem;
    font-size: 20px;
    color: #fff;
    position: relative;
    background: #358cfa;
}
.title span{
    display: block;
    position: absolute;
    width: 60px;
    top: 5px;
    left: 5px;
    line-height: 0.9rem;
    font-size: 18px;
    color: #fff;
    
}
.deta_box{
    width: 100%;
    height: 3rem;
}
.deta_year{
    width: 100%;
    height: 0.9rem;
    background: #f3f3f3;
    box-sizing: border-box;
    border-bottom: 1px solid #ccc;
    font-size: 19px;
    line-height: 0.9rem;
    padding-left: 10px;
}
.deta_month_son{
    width: 100%;
    height: 0.8rem;
    /* background: #ccc; */
    font-size: 18px;
    line-height: 0.8rem;
    box-sizing: border-box;
    border-bottom: 1px solid #CCC;
    padding-left: 10px;
}