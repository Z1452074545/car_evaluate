.title {
    width: 100%;
    height: 1rem;
    text-align: center;
    line-height: 1rem;
    font-size: 20px;
    color: #040404;
    position: relative;
    background: #fff;
}

.title span {
    display: block;
    position: absolute;
    width: 60px;
    top: 5px;
    left: 5px;
    line-height: 0.9rem;
    font-size: 18px;
    color: #040404;

}

.car_title {
    width: 100%;
    height: 1.68rem;
    background: url("../images/carmin_bg.png");
    background-size: 100% 100%;
}

.car_tit_cont {
    width: 94%;
    height: 2rem;
    background: #fff;
    margin: -17px auto 0;
    box-shadow: 3px 2px 9px #ccc;
    border-radius: 0 0 10px 10px;
}

.car_title {
    text-align: center;
    line-height: 1.58rem;
    color: #fff;
    font-size: 20px;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;

}

.car_tit_cont_son {
    float: left;
    width: 25%;
    height: 1.4rem;
    box-sizing: border-box;
    border-right: 1px solid #ccc;
    margin-top: 0.3rem;
}

.car_tit_cont_son:nth-child(4) {
    border-right: none;
}

.car_tit_cont_son div {
    width: 100%;
    height: 50%;
    text-align: center;
    line-height: 0.7rem;
    font-size: 18px;
}

.cont_son_top {
    color: #eb8700;
    font-size: 19px !important;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.cont_son_bottom {
    color: #b8b8b8;
}

.pg_res_son {
    width: 100%;
    height: 3.8rem;
    background: url('../images/car_gu1.png');
    background-size: 100% 100%;
    margin-top: 20px;
    position: relative;
}

.pg_res_son_title {
    width: 90%;
    height: 47%;
    border-bottom: 1px solid #e3e3e3;
    margin: 0 auto;
}

.son_title_top {
    width: 100%;
    height: 45%;
    text-align: center;
    line-height: 0.8rem;
    font-size: 20px;
}

.son_title_bottom {
    width: 100%;
    height: 55%;
    text-align: center;
    font-size: 30px;
    font-weight: 600;
    line-height: 1rem;
    color: #3782f9;
}

.pg_car_left {
    width: 48px;
    height: 62px;
    position: absolute;
    top: -12px;
    left: 12px;
}

.pg_car_left img {
    display: block;
    width: 100%;
    height: 100%;
}

.pg_car_right {
    width: 80px;
    height: 33px;
    background: url('../images/you.png');
    background-size: 100% 100%;
    position: absolute;
    top: 0px;
    right: 0px;
    font-size: 14px;
    color: #fff;
    text-align: right;
    box-sizing: border-box;
    padding-right: 8px;
    line-height: 26px;
}

.lianghao {
    background: url('../images/liang.png') !important;

}

.yiban {
    background: url('../images/yiban.png') !important;

}

.pg_res_son_cont {
    width: 100%;
    height: 53%;
}

.pg_res_son_cont_son {
    width: 25%;
    height: 100%;
    text-align: center;
    font-size: 20px;
}

.cont_son_1 {
    width: 100%;
    height: 0.6rem;
    font-size: 18px;
    line-height: 0.6rem;
    color: #cacaca;
    margin-top: 0.2rem;
}

.cont_son_2 {
    width: 100%;
    height: 0.4rem;
    font-size: 18px;
    line-height: 0.4rem;
    color: #353637;
    margin-top: 0.2rem;
}

.cont_son_3 {
    width: 100%;
    height: 0.4rem;
    font-size: 18px;
    line-height: 0.4rem;
    color: #151616;
    /* margin-top: 0.1rem; */
}

.cont_son_4 {
    width: 100%;
    height: 0.4rem;
    font-size: 18px;
    line-height: 0.4rem;
    color: #151616;
    margin-top: 0.4rem;
}

.pg_res_son_cont>div {
    float: left;
}

.box_col_green {
    width: 100%;
    height: 0.3rem;
    background: #f3f5f8;
}

.interval {
    width: 100%;
    height: 3rem;

    /* margin-top:0.3rem; */
}

.interval_top {
    width: 100%;
    height: 1.5rem;
    font-size: 20px;
    text-align: center;
    line-height: 1.2rem;
    color: rgba(80, 80, 80, 1);
}

.interval_bottom {
    width: 100%;
    height: 1.5rem;
    position: relative;

}

.interval_bottom img {
    display: block;
    width: 100%;
    height: 100%;
}

.interval_bottom>div {
    position: absolute;
    width: 80px;
    height: 30px;
    top: -30px;
    font-size: 16px;
    font-weight: 600;
    text-align: center;
    line-height: 30px;
    color: rgba(55, 130, 249, 1);
}

.interval_left_txt {
    left: 10px;
}

.interval_cont_txt {
    left: 50%;
    margin-left: -40px;
}

.interval_right_txt {
    right: 10px;
}

.trend {
    width: 100%;
    height: 4rem;
}

.trend_title {
    width: 100%;
    height: 1rem;
    text-align: center;
    line-height: 1rem;
    font-size: 20px;
    color: rgba(80, 80, 80, 1);
}

.trend_cont {
    width: 90%;
    height: 3rem;
    box-sizing: border-box;
    border-bottom: 4px solid #Ededed;
    margin: 0 auto;
    position: relative;
}

.trend_cont_img {
    position: absolute;
    top: 48px;
    left: 70px;
    width: 4rem;
    height: 2rem;
    /* background: red; */
    z-index: 99999;
}

.trend_cont_img img {
    width: 100%;
    height: 100%;
    display: block;
}

.trend_cont_son {
    width: 48px;
    height: 2.3rem;
    background: #f3f5f8;
    position: absolute;
    bottom: 0px;
    box-sizing: border-box;
    border: 1px dashed #ccc;
    border-bottom: none;
}

.trend_cont_son1 {
    left: 50px;
}

.trend_cont_son2 {
    left: 50%;
    margin-left: -20px;
}

.trend_cont_son3 {
    right: 50px;
}

.act_son {
    width: 100%;
    height: 100%;
    margin: 0 auto;
    position: absolute;
    bottom: 0;
    background: linear-gradient(139deg, rgba(45, 186, 255, 1) 0%, rgba(60, 106, 247, 1) 100%);
}

.act_son1 {
    height: 100%;
}

.act_son2 {
    height: 75%;
}

.act_son3 {
    height: 55%;
}

.trend_cost {
    width: 80px;
    height: 22px;
    font-size: 16px;
    font-family: PingFangSC-Semibold;
    font-weight: 600;
    color: rgba(170, 170, 170, 1);
    line-height: 22px;
    position: absolute;
    left: 50%;
    top: -28px;
    margin-left: -40px;
}

.trend_bottom_year {
    width: 80px;
    height: 30px;
    font-size: 16px;
    font-family: PingFangSC-Semibold;
    font-weight: 600;
    text-align: center;
    color: rgba(170, 170, 170, 1);
    line-height: 22px;
    position: absolute;
    left: 50%;
    bottom: -38px;
    margin-left: -40px;
}

.expected {
    width: 88%;
    height: 0.7rem;
    box-sizing: border-box;
    margin: 0 auto;
    margin-top: 0.7rem;
    border-radius: 17px;
    border: 1px solid #ff563e;
    font-size: 16px;
    font-family: PingFangSC-Semibold;
    font-weight: 600;
    color: rgba(81, 81, 81, 1);
    text-align: center;
    line-height: 0.7rem;
    position: relative;
}

.expected span {
    color: rgba(245, 103, 35, 1);
}

.expected_img {
    width: 12px;
    height: 16px;
    position: absolute;
    top: 13px;
    right: 56px;
}

.expected_img img {
    display: block;
    width: 100%;
    height: 100%;
}

.buy_cars {
    width: 100%;
    height: 1.6rem;
    margin-top: 0.4rem;
}

.buy_cars img {
    display: block;
    width: 100%;
    height: 100%;
}

.footer_but {
    width: 100%;
    height: 1.6rem;
    margin-top: 0.4rem;
    border-top: 1px solid rgba(230, 230, 230, 1);
}

.footer_but div {
    float: left;
    width: 40%;
    margin-left: 0.5rem;
    height: 0.9rem;
    box-sizing: border-box;
    border-radius: 10px;
    margin-top: 0.4rem;
    line-height: 0.9rem;
    text-align: center;
    border: 1px solid rgba(255, 99, 99, 1);
    font-size: 18px;
    font-family: PingFangSC-Semibold;
    font-weight: 600;
    color: rgba(255, 111, 0, 1);
}