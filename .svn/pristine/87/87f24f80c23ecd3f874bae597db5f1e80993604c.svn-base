<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1,minimum-scale=1,user-scalable=no">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="stylesheet" href="css/common.css">
    <link rel="stylesheet" href="css/results.css">
    <title>估值结果</title>
</head>

<body>
    <!-- 选择时间 start -->
    <div class="max_width brank_alls">
        <div class="title"> <span>返回</span> 评估结果</div>
    </div>
    <div class="max_width car_box">
        <div class="car_title"></div>
        <div class="car_tit_cont">
            <div class="car_tit_cont_son">
                <div class="cont_son_top city">..</div>
                <div class="cont_son_bottom">所在城市</div>
            </div>
            <div class="car_tit_cont_son">
                <div class="cont_son_top regdate">..</div>
                <div class="cont_son_bottom">上牌日期</div>
            </div>
            <div class="car_tit_cont_son">
                <div class="cont_son_top miles">..</div>
                <div class="cont_son_bottom">行驶里程</div>
            </div>
            <div class="car_tit_cont_son">
                <div class="cont_son_top regdate">..</div>
                <div class="cont_son_bottom">出厂年月</div>
            </div>
        </div>

    </div>
    <!-- 评估结果 start-->
    <div class="max_width pg_res">
        <!-- 车况优秀 -->
        <div class="pg_res_son">
            <div class="pg_car_left">
                <img src="./images/car_gu_l.png" alt="" srcset="">
            </div>
            <div class="pg_car_right">
                车况优秀
            </div>
            <div class="pg_res_son_title">
                <div class="son_title_top">估价范围(万元)</div>
                <div class="son_title_bottom">43.00 ~ 46.00</div>
            </div>
            <div class="pg_res_son_cont">
                <div class="pg_res_son_cont_son">
                    <div class="cont_son_1">外观</div>
                    <div class="cont_son_2">无色差</div>
                    <div class="cont_son_3">个别瑕疵</div>
                </div>
                <div class="pg_res_son_cont_son">
                    <div class="cont_son_1">内饰</div>
                    <div class="cont_son_2">轻微磨损</div>
                    <div class="cont_son_3">无异味</div>
                </div>
                <div class="pg_res_son_cont_son">
                    <div class="cont_son_1">里程</div>
                    <div class="cont_son_4">15万公里内</div>

                </div>
                <div class="pg_res_son_cont_son">
                    <div class="cont_son_1">故障维修</div>
                    <div class="cont_son_2">无事故</div>
                    <div class="cont_son_3">少量维修</div>
                </div>

            </div>

        </div>
        <!-- 车况良好 -->
        <div class="pg_res_son">
            <div class="pg_car_right lianghao">
                车况良好
            </div>
            <div class="pg_res_son_title">
                <div class="son_title_top">估价范围(万元)</div>
                <div class="son_title_bottom">43.00 ~ 46.00</div>
            </div>
            <div class="pg_res_son_cont">
                <div class="pg_res_son_cont_son">
                    <div class="cont_son_1">外观</div>
                    <div class="cont_son_2">无色差</div>
                    <div class="cont_son_3">个别瑕疵</div>
                </div>
                <div class="pg_res_son_cont_son">
                    <div class="cont_son_1">内饰</div>
                    <div class="cont_son_2">轻微磨损</div>
                    <div class="cont_son_3">没有异味</div>
                </div>
                <div class="pg_res_son_cont_son">
                    <div class="cont_son_1">里程</div>
                    <div class="cont_son_4">15万公里内</div>

                </div>
                <div class="pg_res_son_cont_son">
                    <div class="cont_son_1">故障维修</div>
                    <div class="cont_son_2">无事故</div>
                    <div class="cont_son_3">少量维修</div>
                </div>

            </div>

        </div>
        <!-- 车况一般 -->
        <div class="pg_res_son">
            <div class="pg_car_right yiban">
                车况一般
            </div>
            <div class="pg_res_son_title">
                <div class="son_title_top">估价范围(万元)</div>
                <div class="son_title_bottom">43.00 ~ 46.00</div>
            </div>
            <div class="pg_res_son_cont">
                <div class="pg_res_son_cont_son">
                    <div class="cont_son_1">外观</div>
                    <div class="cont_son_2">轻微色差</div>
                    <div class="cont_son_3">少量瑕疵</div>
                </div>
                <div class="pg_res_son_cont_son">
                    <div class="cont_son_1">内饰</div>
                    <div class="cont_son_2">个别严重</div>
                    <div class="cont_son_3">破损</div>
                </div>
                <div class="pg_res_son_cont_son">
                    <div class="cont_son_1">里程</div>
                    <div class="cont_son_4">20万公里内</div>

                </div>
                <div class="pg_res_son_cont_son">
                    <div class="cont_son_1">故障维修</div>
                    <div class="cont_son_2">有碰撞事故</div>
                    <div class="cont_son_3">少量维修</div>
                </div>

            </div>

        </div>

    </div>
    <!-- 评估结果 end-->

    <!-- 售价区间 start -->
    <div class="max_width interval">
        <div class="interval_top">售价区间</div>
        <div class="interval_bottom">
            <div class="interval_left_txt">27.02万元</div>
            <div class="interval_cont_txt">27.02万元</div>
            <div class="interval_right_txt">27.02万元</div>
            <img src="./images/qujian.png" alt="" srcset="">
        </div>
    </div>
    <!-- 售价区间 end -->
    <div class="max_width box_col_green"></div>
    <!-- 未来趋势 start -->
    <div class="max_width trend">
        <div class="trend_title">未来趋势</div>
        <div class="trend_cont">
            <div class="trend_cont_img">
                <img src="./images/line.png" alt="" srcset="">
            </div>
            <div class="trend_cont_son trend_cont_son1">
                <div class="act_son act_son1">
                    <div class="trend_cost">22.03万元</div>
                    <div class="trend_bottom_year">2019年</div>
                </div>
            </div>
            <div class="trend_cont_son trend_cont_son2">
                <div class="act_son act_son2">
                    <div class="trend_cost">22.03万元</div>
                    <div class="trend_bottom_year">2020年</div>
                </div>
            </div>
            <div class="trend_cont_son trend_cont_son3">
                <div class="act_son act_son3">
                    <div class="trend_cost">22.03万元</div>
                    <div class="trend_bottom_year">2021年</div>
                </div>
            </div>
        </div>
    </div>
    <div class="max_width expected">
         预计该车型每月贬值<span>3488.21元</span>
         <div class="expected_img">
             <img src="./images/Shape.png" alt="" srcset="">
         </div>
    </div>
    <!-- 未来趋势 end  -->
    <!-- 买车 start -->
    <div class="max_width buy_cars">
        <img src="./images/Bitmap.png" alt="" srcset="">
    </div>
    <!-- 买车 end -->
    <!-- 底部but -->
    <div class="max_width footer_but">
        <div class="footer_but_left">车抵贷</div>
        <div class="footer_but_left">高价售车</div>

    </div>
    <div style="height:2rem; width:100%"></div>
    
    <script src="js/rem.js"></script>
    <script src="js/jquery-2.1.4.js"></script>
    <script src="js/results.js"></script>
</body>

</html>